<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>star</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet-mapping>
     <servlet-name>Report</servlet-name>
     <url-pattern>*.report</url-pattern>
  </servlet-mapping>
  <servlet>
     <servlet-name>Report</servlet-name>
     <servlet-class>com.star.report.ReportFrontController</servlet-class>
  </servlet>

  <servlet-mapping>
  	<servlet-name>Review</servlet-name>
  	<url-pattern>*.review</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>Review</servlet-name>
  	<servlet-class>com.star.review.ReviewFrontController</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>Use</servlet-name>
  	<url-pattern>*.use</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>Use</servlet-name>
  	<servlet-class>com.star.use.UseFrontController</servlet-class>
  </servlet>
   
   <servlet-mapping>
  	<servlet-name>User</servlet-name>
  	<url-pattern>*.user</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>User</servlet-name>
  	<servlet-class>com.star.user.UserFrontController</servlet-class>
  </servlet>

</web-app>